<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.erotictofu.finalproj">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TournamentEvents"
            android:label="@string/title_activity_tournament_events"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewEvents"
            android:label="@string/title_activity_view_events"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".DecksActivity"></activity>
        <activity android:name=".AuthCompleteActivity"
            android:label="AuthCompleteActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>